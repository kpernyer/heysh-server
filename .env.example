# Hey.sh Environment Configuration
# Copy this file to .env and fill in your actual values

# ==================== SUPABASE ====================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_JWT_SECRET=your_jwt_secret

# ==================== NEO4J ====================
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_secure_password

# ==================== AI/ML SERVICES ====================
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=your-anthropic-key

# ==================== TEMPORAL CLOUD ====================
# Only needed for production (local uses docker)
TEMPORAL_CLOUD_ADDRESS=your-namespace.temporal.cloud:7233
TEMPORAL_CLOUD_NAMESPACE=your-namespace
TEMPORAL_CLOUD_CLIENT_CERT=/path/to/client.pem
TEMPORAL_CLOUD_CLIENT_KEY=/path/to/client-key.pem

# ==================== GCP ====================
GCP_PROJECT_ID=hey-sh-production
GCP_REGION=us-central1
GCP_ZONE=us-central1-a

# ==================== DATABASE ====================
DATABASE_URL=postgresql://user:password@postgres:5432/hey_sh

# ==================== LOCAL DEVELOPMENT ====================
DEBUG=false
LOG_LEVEL=info
